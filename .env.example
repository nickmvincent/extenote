# Extenote Environment Configuration
# Copy this file to .env and fill in your values

# Content roots (required)
EXTENOTE_CONTENT_ROOT=../extenote-pub/content
EXTENOTE_PRIVATE_ROOT=../extenote-priv

# ATProto/Semble sync (optional - for publishing to Bluesky)
# SEMBLE_APP_PASSWORD=your_atproto_app_password

# GitHub discussions integration (optional)
# GITHUB_TOKEN=your_github_personal_access_token

# FTP deployment (required for personal-website deploy)
# FTP_HOST=ftp.example.com
# FTP_USER=user@example.com
# FTP_PASSWORD=your_ftp_password
